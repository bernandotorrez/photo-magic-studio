{
  "info": {
    "name": "Photo Enhancement API",
    "description": "API untuk generate gambar produk dengan AI enhancement",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "value",
        "value": "{{API_KEY}}",
        "type": "string"
      },
      {
        "key": "key",
        "value": "x-api-key",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "BASE_URL",
      "value": "https://your-project-id.supabase.co/functions/v1",
      "type": "string"
    },
    {
      "key": "API_KEY",
      "value": "eak_your_api_key_here",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Generate Enhanced Image",
      "item": [
        {
          "name": "Basic Generation",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"imageUrl\": \"https://images.unsplash.com/photo-1523381210434-271e8be1f52b\",\n  \"enhancement\": \"add_female_model\",\n  \"classification\": \"clothing\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api-generate",
              "host": ["{{BASE_URL}}"],
              "path": ["api-generate"]
            },
            "description": "Generate gambar dengan enhancement dasar"
          },
          "response": []
        },
        {
          "name": "With Text Watermark",
