    {
      "name": "Error Cases",
      "item": [
        {
          "name": "Missing API Key",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"imageUrl\": \"https://images.unsplash.com/photo-1523381210434-271e8be1f52b\",\n  \"enhancement\": \"add_female_model\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api-generate",
              "host": ["{{BASE_URL}}"],
              "path": ["api-generate"]
            },
            "description": "Test error ketika API key tidak disertakan"
          },
          "response": []
        },
        {
          "name": "Invalid API Key",
          "request": {
            "auth": {
              "type": "apikey",
              "apikey": [
                {
                  "key": "value",
                  "value": "invalid_key",
                  "type": "string"
                },
                {
                  "key": "key",
                  "value": "x-api-key",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"imageUrl\": \"https://images.unsplash.com/photo-1523381210434-271e8be1f52b\",\n  \"enhancement\": \"add_female_model\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api-generate",
              "host": ["{{BASE_URL}}"],
              "path": ["api-generate"]
            },
            "description": "Test error dengan API key yang salah"
          },
          "response": []
        },
        {
          "name": "Missing Required Fields",
          "request": {
            "method": "POST",
